{
  "expo": {
    "name": "DB2B",
    "slug": "DB2B",
    "version": "1.0.0",
    "scheme": "DB2B",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "strech",
      "backgroundColor": "#A734C4"
    },
    "platforms": [
      "android",
      "ios"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.db2b.myapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1e88b248-eca4-4efb-a446-6c039a076f5a"
      }
    }
  }
}
